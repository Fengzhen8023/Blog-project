<% if(theme.social){%>
<div class="widget">
    <h3 class="title">
        <%= __('social') %>
    </h3>
    <div class="content social">
        <% theme.social.forEach(function (item) {%>
        <a href="<%- item.href%>" rel="external nofollow" title="<%- item.name%>" target="_blank" style="text-decoration: none;">
            <span class="iconfont">&#<%- item.icon%></span>
        </a>
        <%})%>
    </div>
</div>
<%}%>